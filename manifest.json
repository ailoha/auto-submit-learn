{
  "manifest_version": 3,
  "name": "Auto Submit Learn",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": ["https://xfks-study.gdsf.gov.cn/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["https://xfks-study.gdsf.gov.cn/*"]
    }
  ]
}
